
{% extends 'template.html.twig' %}

{% block body %}
    {{ form_start(form,{'attr': {'novalidate': 'novalidate'}}) }}
    <div class="row">
        <div class="col">{{ form_row(form.droitFixe) }}</div>
        <div class="col">{{ form_row(form.droitProportionnel) }}</div>
        <div class="col">{{ form_row(form.valeurLocative) }}</div>
    </div>
    <div class="row">
        <div class="col">{{ form_row(form.tauValeurLocative )}}</div>
        <div class="col">{{ form_row(form.date )}}</div>
    </div>
    {{ form_rest(form) }}
{% endblock %}
{% block javascripts %}
    <script>
        $(document).ready(function() {
            $('.select2').select2();
        });
    </script>
{% endblock %}

